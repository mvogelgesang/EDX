{
  "name": "edx_cli",
  "description": "oclif based CLI to evaluate web properties",
  "version": "0.0.20",
  "author": "US General Services Administration",
  "bin": {
    "edxcli": "./bin/run"
  },
  "bugs": "https://github.com/gsa/edx/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "airtable": "^0.11.4",
    "axios": "^0.26.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.0",
    "fast-fuzzy": "^1.11.2",
    "lighthouse": "^9.2.0",
    "lodash": "^4.17.21",
    "puppeteer": "^14.3.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.21",
    "@types/puppeteer": "^5.4.6",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "globby": "^11",
    "mocha": "^9",
    "nock": "^13.2.4",
    "nyc": "^15.1.0",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/gsa/edx",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "edxcli",
    "dirname": "edxcli",
    "commands": "./dist/commands",
    "hooks": {
      "state_manager:create": "./dist/hooks/state_manager/create",
      "state_manager:delete": "./dist/hooks/state_manager/delete",
      "state_manager:retrieve": "./dist/hooks/state_manager/retrieve",
      "state_manager:update": "./dist/hooks/state_manager/update"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "gsa/edx",
  "mocha": {
    "spec": [
      "test/**/*.test.ts"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "npx nyc@latest mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
